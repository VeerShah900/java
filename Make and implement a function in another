class maths
{
    void mat(){
        int maxInt = max(20,40,100);
        System.out.println(maxInt);
      
    }
    
    //Our max() function
  private void max(int n1,int n2,int n3){
     if(n1>n2 && n1 > n3)
   {
     System.out.println(n1);
  }
      if(n2>n1 && n2 > n3)
       {
          System.out.println(n2);
       }
       if(n3 >n1 && n3 > n2)
       {
           System.out.println(n3);
       }
    }
}
