import java.util.Scanner;
public class module3_chapter2 {
    public static void main(String[] args) {
        System.out.println("Enter Age");
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
        if(age < 100){System.out.println("Hi " + "I guess your age is " + age);}
        else {
            System.out.println("Unable to trick the system try again");};
        System.out.println("How was my program 1:good 2:bad 3)worst");
        int name = sc.nextInt();
        if (name > 3) {
            System.out.println("OO out of box thank you");
        }

        if (name == 1) {
            System.out.println("Thanks I know i  am the best");
        }
        if (name == 2) {
            System.out.println("OO But I know this program is the best");}
        if (name == 3){
            System.out.println("A comment to you I know I am the best");
        }


        if (age > 100 || name > 3){
            System.out.println("Enter proper age or number")
    }

 }}
